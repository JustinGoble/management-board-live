{
  "name": "havoc-nexus",
  "version": "1.0.0",
  "description": "HAVOC's tool for managing industry operations",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/havoc-tools/havoc-nexus"
  },
  "engines": {
    "node": "^16.15.0",
    "npm": "^8.5.5"
  },
  "scripts": {
    "start": "npm-run-all react-build node-start",
    "start-prod": "npm-run-all node-start",
    "build": "npm-run-all react-build",
    "watch": "npm-run-all --parallel node-watch react-start",
    "deploy": "npm-run-all migrate node-start",
    "test": "bash ./tools/test-local.sh",
    "test-ci": "bash ./tools/test-ci.sh",
    "lint": "npm run eslint",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "mocha": "mocha --exit --timeout 5000 ./tests/**/*.test.js",
    "eslint": "eslint backend discord-bot fixtures migrations src tests server.js knexfile.js",
    "docker-recompose": "docker-compose down && docker-compose up -d",
    "node-start": "node ./server.js",
    "node-watch": "nodemon -e js,json,yaml ./server.js",
    "react-analyze": "source-map-explorer 'build/static/js/*.js'",
    "react-start": "react-app-rewired start",
    "react-build": "react-app-rewired build",
    "react-test": "react-app-rewired test --env=jsdom",
    "heroku-postbuild": "npm run react-build",
    "make-member": "node ./tools/make-member.js",
    "make-manager": "node ./tools/make-manager.js",
    "make-admin": "node ./tools/make-admin.js",
    "seed": "node ./tools/seed-database.js",
    "add-test-user": "node ./tools/add-user.js",
    "test-bot": "node ./discord-bot/local-test.js"
  },
  "dependencies": {
    "@ant-design/icons": "latest",
    "@discordjs/opus": "^0.1.0",
    "antd": "^4.10.2",
    "axios": "^0.21.1",
    "babel-plugin-import": "^1.13.0",
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "colors": "^1.4.0",
    "common-tags": "^1.8.0",
    "cors": "^2.8.4",
    "customize-cra": "^0.5.0",
    "discord.js": "^12.4.1",
    "express": "^4.16.3",
    "express-rate-limit": "^3.1.1",
    "ffmpeg-static": "^3.0.0",
    "form-data": "^3.0.0",
    "googleapis": "^46.0.0",
    "history": "^4.7.2",
    "joi": "^14.3.1",
    "json-refs": "^3.0.10",
    "knex": "0.95.2",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.20",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "pg": "^8.5.1",
    "react": "16.14.0",
    "react-app-rewired": "2.1.6",
    "react-dom": "16.14.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "^3.4.3",
    "redis": "^2.8.0",
    "source-map-explorer": "^2.5.0",
    "swagger-ui-express": "^4.0.1",
    "unstated": "^2.1.1",
    "useragent": "^2.3.0",
    "uuid": "^3.3.2",
    "winston": "^2.4.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-jest-snapshot": "^2.0.0",
    "eslint": "^6.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.20.6",
    "mocha": "^7.2.0",
    "nock": "^11.7.0",
    "sinon": "^9.0.3",
    "supertest": "^4.0.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
